<%- include ../layouts/header.ejs %>

<% if (messages.error) { %>
 <p style="color:red"><%- messages.error %></p>
<% } %>

<% if (messages.success) { %>
 <p style="color:green"><%- messages.success %></p>
<% } %>

<table>
 <tr>
  <th>Carbs</th>
  <th>Fat</th>
  <th>Item No</th>
  <th>Protein</th> 
  <th>Sodium</th>   
 </tr> 

 <% if (data) { %>
 <% data.forEach(function(user){ %>
  <tr>
   <td><%= user.Carbs %></td>
   <td><%= user.Fat %></td>
   <td><%= user.Nc_item_no %></td>
   <td><%= user.Protein %></td>
   <td><%= user.Sodium %></td>
  </tr>
 <% }) %>
 <% } %>
</table>

<%- include ../layouts/footer.ejs %>
